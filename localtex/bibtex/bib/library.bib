file = {::},
issn = {0009-3920},
journal = {Child development},
keywords = {Age Factors,Child Psychology,Child, Preschool,Comprehension,Concept Formation,Female,Form Perception,Humans,Interview, Psychological,Language Development,Male,Problem Solving,Set (Psychology),Stereotyped Behavior,Verbal Behavior,Verbal Learning},
