@article{Lee2004,
abstract = {The gap/gapless processing debate in the psycholinguistics literature contrasts two processing models: one that assumes the trace-based Government and Binding (or Principles and Parameters) Grammar and the (augmented) Active Filler Strategy and one that assumes the traceless Dependency Categorial Grammar and the Principle of Dependency Formation. This paper reports on an experiment that found new evidence against the gapless/traceless model, considers why such evidence was not found in previous studies, and explores whether a parser that combines a (partially) traceless grammar and the augmented Active Filler Strategy can explain the current finding.},
author = {Lee, Ming-Wei},
file = {::},
